{"ast":null,"code":"export default {\n  name: 'RealFoot',\n  // Nombre del componente\n  data() {\n    return {\n      whatsappNumber: '+56989920340',\n      // Número de WhatsApp para contacto\n      instagramLink: 'https://www.instagram.com/realacademyf.c',\n      // Enlace al perfil de Instagram\n      whatsappLink: 'https://wa.me/56989920340',\n      // Enlace directo para iniciar chat en WhatsApp\n      linkedinLink: 'https://www.linkedin.com/in/rodrigo-lagos-fernandez-403a33173/' // Enlace al perfil de LinkedIn\n    };\n  },\n  mounted() {\n    // @ts-ignore\n    const footer = this.$refs.footer.querySelector('.social-icons');\n\n    // IntersectionObserver para aplicar animaciones al scrollear\n    const observer = new IntersectionObserver(entries => {\n      entries.forEach(entry => {\n        if (entry.isIntersecting) {\n          entry.target.classList.add('visible');\n        } else {\n          entry.target.classList.remove('visible');\n        }\n      });\n    });\n    observer.observe(footer);\n  },\n  methods: {\n    // Método para mostrar alerta de Facebook\n    showFacebookAlert() {\n      alert('Red social en creación'); // Mensaje de alerta al hacer clic en el icono de Facebook\n    }\n  }\n};","map":{"version":3,"names":["name","data","whatsappNumber","instagramLink","whatsappLink","linkedinLink","mounted","footer","$refs","querySelector","observer","IntersectionObserver","entries","forEach","entry","isIntersecting","target","classList","add","remove","observe","methods","showFacebookAlert","alert"],"sources":["C:\\Users\\DIVDOC-INTERNET\\Desktop\\real-academy\\real-academy-frontend\\src\\components\\RealFoot.vue"],"sourcesContent":["<template>\r\n  <footer class=\"real-foot\" ref=\"footer\">\r\n    <div class=\"social-icons\">\r\n      <!-- Enlace a WhatsApp -->\r\n      <a :href=\"whatsappLink\" target=\"_blank\" rel=\"noopener noreferrer\" class=\"social-icon\">\r\n        <i class=\"fab fa-whatsapp\"></i>\r\n      </a>\r\n      <!-- Enlace a Instagram -->\r\n      <a :href=\"instagramLink\" target=\"_blank\" rel=\"noopener noreferrer\" class=\"social-icon\">\r\n        <i class=\"fab fa-instagram\"></i>\r\n      </a>\r\n      <!-- Alerta de Facebook (en construcción) -->\r\n      <a @click=\"showFacebookAlert\" href=\"javascript:void(0)\" rel=\"noopener noreferrer\" class=\"social-icon\">\r\n        <i class=\"fab fa-facebook-f\"></i>\r\n      </a>\r\n      <!-- Enlace a LinkedIn -->\r\n      <a :href=\"linkedinLink\" target=\"_blank\" rel=\"noopener noreferrer\" class=\"social-icon\">\r\n        <i class=\"fab fa-linkedin\"></i>\r\n      </a>\r\n    </div>\r\n  </footer>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'RealFoot', // Nombre del componente\r\n  data() {\r\n    return {\r\n      whatsappNumber: '+56989920340', // Número de WhatsApp para contacto\r\n      instagramLink: 'https://www.instagram.com/realacademyf.c', // Enlace al perfil de Instagram\r\n      whatsappLink: 'https://wa.me/56989920340', // Enlace directo para iniciar chat en WhatsApp\r\n      linkedinLink: 'https://www.linkedin.com/in/rodrigo-lagos-fernandez-403a33173/' // Enlace al perfil de LinkedIn\r\n    };\r\n  },\r\n  mounted() {\r\n    // @ts-ignore\r\n    const footer = this.$refs.footer.querySelector('.social-icons');\r\n\r\n    // IntersectionObserver para aplicar animaciones al scrollear\r\n    const observer = new IntersectionObserver((entries) => {\r\n      entries.forEach(entry => {\r\n        if (entry.isIntersecting) {\r\n          entry.target.classList.add('visible');\r\n        } else {\r\n          entry.target.classList.remove('visible');\r\n        }\r\n      });\r\n    });\r\n\r\n    observer.observe(footer);\r\n  },\r\n  methods: {\r\n    // Método para mostrar alerta de Facebook\r\n    showFacebookAlert() {\r\n      alert('Red social en creación'); // Mensaje de alerta al hacer clic en el icono de Facebook\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.real-foot {\r\n  padding: 20px;\r\n  background-color: #000;\r\n  color: white;\r\n  text-align: center;\r\n}\r\n\r\n.social-icons {\r\n  display: flex;\r\n  justify-content: center;\r\n  gap: 60px;\r\n  opacity: 0;\r\n  transform: translateY(20px);\r\n  transition: opacity 0.6s ease-out, transform 0.6s ease-out;\r\n}\r\n\r\n.social-icons.visible {\r\n  opacity: 1;\r\n  transform: translateY(0);\r\n}\r\n\r\n.social-icons a {\r\n  color: white;\r\n  font-size: 2.8em;\r\n}\r\n\r\n.social-icons a:hover {\r\n  color: #FF007F;\r\n}\r\n</style>\r\n"],"mappings":"AAwBA,eAAe;EACbA,IAAI,EAAE,UAAU;EAAE;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,cAAc;MAAE;MAChCC,aAAa,EAAE,0CAA0C;MAAE;MAC3DC,YAAY,EAAE,2BAA2B;MAAE;MAC3CC,YAAY,EAAE,gEAA+D,CAAE;IACjF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR;IACA,MAAMC,MAAK,GAAI,IAAI,CAACC,KAAK,CAACD,MAAM,CAACE,aAAa,CAAC,eAAe,CAAC;;IAE/D;IACA,MAAMC,QAAO,GAAI,IAAIC,oBAAoB,CAAEC,OAAO,IAAK;MACrDA,OAAO,CAACC,OAAO,CAACC,KAAI,IAAK;QACvB,IAAIA,KAAK,CAACC,cAAc,EAAE;UACxBD,KAAK,CAACE,MAAM,CAACC,SAAS,CAACC,GAAG,CAAC,SAAS,CAAC;QACvC,OAAO;UACLJ,KAAK,CAACE,MAAM,CAACC,SAAS,CAACE,MAAM,CAAC,SAAS,CAAC;QAC1C;MACF,CAAC,CAAC;IACJ,CAAC,CAAC;IAEFT,QAAQ,CAACU,OAAO,CAACb,MAAM,CAAC;EAC1B,CAAC;EACDc,OAAO,EAAE;IACP;IACAC,iBAAiBA,CAAA,EAAG;MAClBC,KAAK,CAAC,wBAAwB,CAAC,EAAE;IACnC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}